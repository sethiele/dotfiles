# git autocomplete
[[ -s $HOME/git-completion.sh ]] && source $HOME/.git-completion.sh

#Color
export CLICOLOR=1
export LSCOLORS=ExFxCxDxBxegedabagacad

# rbnv
eval "$(rbenv init -)"

### Promt design ###
export PS1="\[\033[01;34m\]\u@\h\[\033[00m\] - \e[33m\]\D{%d.%m.%y %T}\[\033[00m\] - \[\033[0;32m\][\w]\[\033[m\] \[\033[1;31m\]$(__git_ps1 "- (%s)")\[\033[m\]\n\$ "; 
